<h2 class="page-header">Editar Author</h2>
<form action="/authors/<%= author.id %>?_method=PUT" method="POST">
  <%- include('_forms_fields') %>
  <div class="form-row form-row-end btn-row">
    <a class="btn btn-danger" href="/authors<%= author.id %>">Cancelar</a>
    <button class="btn btn-primary" type="submit">Actualizar</button>
  </div>
</form>
